{
  "name": "aurelia-types-installer",
  "version": "0.1.3",
  "bin": {
    "ati": "./bin/ati"
  },
  "description": "A utiltiy for Aurelia + TypeScript + JSPM users that installs exact .d.ts versions as external modules.",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublish": "npm run test && npm run build",
    "cleantestapp": "rimraf test/testapp/types && rimraf test/testapps/tsconfig.paths.json",
    "clean": "rimraf dist && npm run cleantestapp",
    "test": "node dist/index.js i --projectDir ./test/testapp --dest jspm_packages/npm",
    "pretest": "npm run build",
    "posttest": "npm run cleantestapp"
  },
  "main": "dist/index",
  "author": "aluanh@gmail.com",
  "homepage": "https://github.com/aluanhaddad/aurelia-types-installer",
  "bugs": {
    "url": "https://github.com/aluanhaddad/aurelia-types-installer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aluanhaddad/aurelia-types-installer"
  },
  "license": "MIT",
  "dependencies": {
    "@types/minimist": "^1.1.28",
    "@types/mz": "0.0.28",
    "@types/request": "0.0.30",
    "@types/request-promise": "^3.0.30",
    "async": "^2.0.1",
    "minimist": "^1.2.0",
    "mz": "^2.4.0",
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "typescript": "2.1.0-dev.20160927",
    "@types/tape": "^4.2.27",
    "jspm": "*",
    "tape": "^4.6.0",
    "rimraf": "2.5.4"
  },
  "keywords": [
    "aurelia",
    "au",
    "jspm",
    "types",
    "typings",
    "typescript",
    "external",
    "modules"
  ]
}