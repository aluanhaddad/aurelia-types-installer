{
  "name": "aurelia-types-installer",
  "version": "0.4.2",
  "bin": {
    "ati": "./bin/ati"
  },
  "description": "A utiltiy for Aurelia + TypeScript + JSPM users that installs exact .d.ts versions as external modules.",
  "scripts": {
    "prepare": "npm run lint && npm test && npm run build",
    "clean": "rimraf dist && npm run pretest",
    "build": "npm run clean && tsc -v && tsc -p tsconfig.json",
    "test": "ts-node -P tsconfig.test.json src/index.ts i --projectDir ./test/testapp --dest jspm_packages/npm",
    "pretest": "rimraf test/testapp/jspm_packages/npm/**/*.ts test/testapp/tsconfig.paths.json && cd test/testapp && jspm i",
    "lint": "tslint --project tsconfig.json --config tslint.json --format stylish"
  },
  "main": "dist/index",
  "typings": "dist/index",
  "author": "aluanh@gmail.com",
  "homepage": "https://github.com/aluanhaddad/aurelia-types-installer",
  "bugs": {
    "url": "https://github.com/aluanhaddad/aurelia-types-installer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aluanhaddad/aurelia-types-installer"
  },
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/chalk": "^2.2.0",
    "@types/deep-equal": "^1.0.1",
    "@types/minimist": "1.2.0",
    "@types/mz": "0.0.32",
    "@types/request": "2.0.7",
    "@types/request-promise": "^4.1.39",
    "async": "^2.6.0",
    "chalk": "^2.3.0",
    "deep-equal": "^1.0.1",
    "flat-map-polyfill": "^0.3.8",
    "minimist": "^1.2.0",
    "mz": "^2.7.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/tape": "^4.2.31",
    "jspm": "^0.17.0-beta.47",
    "rimraf": "2.6.2",
    "source-map-support": "^0.5.0",
    "tape": "^4.8.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "next"
  },
  "keywords": [
    "aurelia",
    "au",
    "jspm",
    "types",
    "typings",
    "typescript",
    "external",
    "modules"
  ]
}
