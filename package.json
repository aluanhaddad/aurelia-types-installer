{
  "name": "aurelia-types-installer",
  "version": "0.1.6",
  "bin": {
    "ati": "./bin/ati"
  },
  "description": "A utiltiy for Aurelia + TypeScript + JSPM users that installs exact .d.ts versions as external modules.",
  "scripts": {
    "start": "tsc -p -w tsconfig.json",
    "prepublish": "npm run build && npm run lint && npm run test",
    "clean": "rimraf dist && rimraf test/testapp/jspm_packages/npm/**/*.ts && rimraf test/testapps/tsconfig.paths.json",
    "build": "npm run clean && tsc -p tsconfig.json",
    "test": "node dist/index.js i --projectDir ./test/testapp --dest jspm_packages/npm",
    "lint": "npm run build && tslint --project tsconfig.json --config tslint.json --format stylish"
  },
  "main": "dist/index",
  "author": "aluanh@gmail.com",
  "homepage": "https://github.com/aluanhaddad/aurelia-types-installer",
  "bugs": {
    "url": "https://github.com/aluanhaddad/aurelia-types-installer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aluanhaddad/aurelia-types-installer"
  },
  "license": "MIT",
  "dependencies": {
    "@types/minimist": "^1.1.29",
    "@types/mz": "0.0.30",
    "@types/request": "0.0.34",
    "@types/request-promise": "^4.1.33",
    "async": "^2.1.4",
    "minimist": "^1.2.0",
    "mz": "^2.6.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "@types/tape": "^4.2.28",
    "jspm": "*",
    "rimraf": "2.5.4",
    "tape": "^4.6.3",
    "tslint": "^4.0.2",
    "typescript": "next"
  },
  "keywords": [
    "aurelia",
    "au",
    "jspm",
    "types",
    "typings",
    "typescript",
    "external",
    "modules"
  ]
}